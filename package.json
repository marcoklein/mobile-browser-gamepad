{
    "name": "mobile-browser-gamepad",
    "version": "0.1.6",
    "description": "Mobile HTML5 web gamepad for the smartphone.",
    "main": "index.js",
    "scripts": {
        "dev": "concurrently \"copyfiles -u 1 src/assets/* dist\"  \"parcel src/index.html src/server.html --open\"",
        "build": "concurrently \"copyfiles -u 1 src/assets/* docs\" \"parcel build src/index.html src/server.html -d docs --public-url ./\"",
        "version:patch": "npm version patch"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/marcoklein/mobile-browser-gamepad.git"
    },
    "keywords": [
        "Gamepad",
        "Mobile",
        "Web",
        "HTML5",
        "Smartphone"
    ],
    "author": "Marco Klein",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/marcoklein/mobile-browser-gamepad/issues"
    },
    "homepage": "https://github.com/marcoklein/mobile-browser-gamepad#readme",
    "devDependencies": {
        "@types/jquery": "^3.3.29",
        "@types/peerjs": "0.0.30",
        "@vue/component-compiler-utils": "^3.0.0",
        "concurrently": "^4.1.0",
        "copyfiles": "^2.1.0",
        "parcel-bundler": "^1.12.3",
        "pug": "^2.0.3",
        "sass": "^1.20.1",
        "typescript": "^3.4.5",
        "vue-class-component": "^7.1.0",
        "vue-property-decorator": "^8.1.1",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "bootstrap": "^4.3.1",
        "bootstrap-vue": "^2.0.0-rc.20",
        "eventemitter3": "^3.1.2",
        "jquery": "^3.4.1",
        "peerjs": "^1.0.0",
        "screenfull": "^4.2.0",
        "vue": "^2.6.10",
        "vue-hot-reload-api": "^2.3.3"
    }
}
